#include <iostream>
#include <climits>
#include <limits.h>

using namespace std;

int main() {

	int value = 54656;
	cout << value << endl;

	cout << "Max int value: " << INT_MAX << endl;
	cout << "Min int value: " << INT_MIN << endl;

	int lValue = 2345325345345;
	cout << lValue << endl;

	return 0;
}
